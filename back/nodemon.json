{
  "restartable": "rs",
  "ignore": [".git", "node_modules/**/node_modules", "src/generated"],
  "verbose": true,
  "execMap": {
    "ts": "node --require ts-node/register"
  },
  "watch": ["src/"],
  "env": {
    "NODE_ENV": "development"
  },
  "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src",
  "ext": "js,ts,tsx"

} 